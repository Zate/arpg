<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>arpg</title>
    <link href="https://fonts.googleapis.com/css?family=Roboto+Mono|Roboto:400,700" rel="stylesheet">
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-33447106-8', 'auto');
      ga('send', 'pageview');

    </script>
</head>
<body>
<div id="leaderboard" class="board" style="display: none;">
    <div id="leaderboard-title" class="board-title">Current game ranks</div>
    <table id="leaderboard-head" class="board-items board head">
        <tbody>
        </tbody>
    </table>
    <table id="leaderboard-tail" class="board-items board tail" style="display: none;">
        <tbody>
            <tr style="display: none;">
                <td class="rank-position">&hellip;</td>
                <td class="name"></td>
                <td class="score"></td>
            </tr>
            <tr id="current-user-rank">
                <td class="rank-position"></td>
                <td class="name"></td>
                <td class="score"></td>
            </tr>
        </tbody>
    </table>
</div>
<div id="hud-debug" class="hud-debug" style="display: none;">
    <dl class="dl-horizontal">
        <dt>FPS</dt>
        <dd data-type="fps"></dd>
        <dt>Ships</dt>
        <dd data-type="ships"></dd>
        <dt>Ping</dt>
        <dd data-type="ping"></dd>
        <dt class="debug">Speed</dt>
        <dd class="debug" data-type="speed"></dd>
        <dt class="debug">X</dt>
        <dd class="debug" data-type="coordinate-x"></dd>
        <dt class="debug">Y</dt>
        <dd class="debug" data-type="coordinate-y"></dd>
    </dl>
</div>
<div id="scoreboard" class="board" style="bottom: 0px; display: none;">
    <div id="scoreboard-title" class="board-title">Highscores</div>
    <table id="scoreboard-items" class="board-items">
        <tbody>
        </tbody>
    </table>
</div>

<div id="game"></div>
</body>
</html>
